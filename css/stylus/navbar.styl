
.navbar
    position: absolute
    width: 100%
    z-index: 1
    .compress
        height: 120px
        display: flex
        justify-content: space-between
        align-items: center
        .logo
            height: 48px
            img
                height: 100%
        .links
            display: flex
            a
                color: $white
                font-size: 20px
                margin-right: 40px
                &:last-child
                    margin-right: 0
